
<%= form_for @exam do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
 
  <p>
    <%= f.label :section %><br>
    <%= f.text_area :section %>
  </p>
  <p>
    <%= f.label :level %><br>
    <%= f.text_area :level %>
  </p>
  <p>
    <%= f.label :exam_date %><br>
    <%= f.text_area :exam_date %>
  </p>
  <p>
    <%= f.file_field :image %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>